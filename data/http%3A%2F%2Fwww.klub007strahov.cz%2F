    <script type="text/javascript">
    function rozbalit(cislo,vice,skryt) {
            cesta = document.getElementById(cislo).style.display;
            if(cesta == "none") {
                document.getElementById(cislo).style.display = "block";
                document.getElementById(vice).style.display = "none";
                document.getElementById(skryt).style.display = "inline";
                }
            if(cesta == "block") {
                document.getElementById(cislo).style.display = "none"
                document.getElementById(vice).style.display = "inline";
                document.getElementById(skryt).style.display = "none";
                }
      }
    </script>

