					var setupSmallCatLayer = function() {
					
						var tmpLayer, messerureElement, tmpColumns, smallCatLayerBtn;
						
						if(!(smallCatLayerBtn = jQuery('#btnfilterKat').get(0) || false)) return;
						
						// Erster Event ohne Verzoegerung, Layer nur durchsichtig machen, damit man die vorzunehmende
						// Anpassung nicht sieht.
						jQuery(smallCatLayerBtn).bind('mouseup', function() {
						
							// Wurde die Hoehe schon berechnet, dann abbrechen.
							if((smallCatLayerBtn.resingColumnsDone || false)) return true;
						
							if(!(tmpLayer = jQuery('#filterKat').get(0) || false)) return;
							
							// Layer durchsichtig, nicht unsichtbar - also weg - machen!
							jQuery(tmpLayer).css({ 'visibility' : 'hidden' });
						});
							
						jQuery(smallCatLayerBtn).bind('mouseup', function() {
							
							// Wurde die Hoehe schon berechnet, dann abbrechen.
							if((smallCatLayerBtn.resingColumnsDone || false)) return true;
							
							var tmpLayer, messerureElement, tmpColumns;
						
							if(!(tmpLayer = jQuery('#filterKat').get(0) || false)) return;
							if(!(messerureElement = jQuery(tmpLayer).find('ul.columnsContainer').get(0) || false)) return;
							if(!(tmpColumns = jQuery(tmpLayer).find('li.column').get() || false)) return;
							
							// Hoehe auf "automatisch" stellen, damit sich der Layer der Hoehe seines Inhaltes anpassen kann.
							jQuery(tmpLayer).css({ 'height' : 'auto' });
							
							// Hoehe auf "automatisch" stellen, damit die durch die Elemente selbst erzeugte Hoehe gemessen werden kann.
							jQuery(tmpColumns).css({ 'height' : 'auto' });
							
							// Hoehe der Columns nun auf die gemessene Hoehe des Messelements setzen.
							jQuery(tmpColumns).height(jQuery(messerureElement).height());
							
							var tmpWidth = 0;
							
							// Breite aller Columns messen, dabei +1 Pixel fuer Rahmen einkalkulieren
							jQuery(tmpColumns).each(function() { tmpWidth += jQuery(this).width() + 1; });
							
							// Layerbreite setzen und sichtbar machen!
							jQuery(tmpLayer).width(tmpWidth)
							
							jQuery(tmpLayer).css({ 'visibility' : 'visible' });
							
							smallCatLayerBtn.resingColumnsDone = true;
							
						}.defer(10));
					};
					
					setupSmallCatLayer();
					
				//-->
