			<script language="javascript" type="text/javascript">	
					// changing the background of the filter box
					// by click on the "search by artist name"							
					function checkClass(){	
						if(jQuery("#eventSearchContainerBtn").attr("class") == "eveJsDone"){							
							jQuery("#filter").css('background','transparent url(/obj/advanced/default/layout/img/backgrounds/filter_trans_milky_white.png) no-repeat scroll 0 0');						
						}else{
							jQuery("#filter").css('background','transparent url(/obj/advanced/default/layout/img/backgrounds/filter_trans_milky.png) no-repeat scroll 0 0');
						}
					}				
			</script>
			
									
						
				
																																																								
						
				
																																																								
						
																								



		
		
			
						
									
							
						
						
					
							
						
									
							
						
						
					
							
						
									
							
						
						
					
							
						
									
							
						
						
					
							
						
									
							
						
						
					
							
						
									
							
						
						
					
							
						
									
							
						
						
					
						
		
		
					
				
						
						
						
												
								
												
								
												
								
																			
						
												
								
												
								
												
								
																			
						
												
								
												
								
												
								
															
																
						
												
								
												
								
												
								
																			
						
												
																			
						
												
																			
						
												
																			
						
