<script type="text/javascript" language="JavaScript">
<!--

	var filterLanguage = function(text) {

		var params = ((text || '').split('?')).pop().split('&');
		
		for(var i in params) {

			var pair = params[i].split('=');
			if((pair[0] || '').toUpperCase() == "LANGUAGE") return pair[1] || false;
		};

		return false;
	};
	
	var switchLanguage = function() {

		// Language filterm
		var language = filterLanguage((this.getAttribute('href') || ''));
		
		// Wurde keine gefunden, dann einfach normal fortfahren.
		if(!language) return;
		
		// ein persistentes language cookie setzen, damit die Sprache vorausgewählt ist, auch wenn der
		// Browser zwischendurch geschlossen wird
		var expirationDate = new Date();	
		expirationDate.setDate(expirationDate.getDate() + 30);
		var expires = expirationDate.toGMTString();
		
		var locationHost = (document.location.host).split(".");
		var helpDomain = locationHost[1]+"."+locationHost[2];
		if(locationHost[2] == "co") helpDomain = helpDomain+"."+locationHost[3];
		
		document.cookie = "language="+language+"; expires="+expires+"; domain="+helpDomain;	
				
		
		
		
												
			var currentURL = window.location.href || '';
			var callUrl = false;
			var splitter = "?";
			
			if(window.location.search != "") {
				
				var isLanguageInHref = filterLanguage(currentURL);

				if(isLanguageInHref) {

					var regexp = new RegExp("language=" + isLanguageInHref, "gi");
					callUrl = currentURL.replace(regexp, "language="+language);
				}
				
				splitter = "&";	
			}

			if(!callUrl) callUrl = currentURL + splitter + 'language=' + language;

			// Sicherstellen, dass immer auf eine Seite und nicht einfach nur auf die
			// Domain geleitet wird, da AKAMAI die Startseite cached.
			var currentPathname = window.location.pathname || '';
			if (currentPathname == '/' || currentPathname == '') callUrl = callUrl.replace('//' + window.location.hostname + currentPathname, '//' + window.location.hostname + '/tickets.html');

			window.location.href = callUrl;
			
		
		return false;
	};

	jQuery(function() {
		jQuery('#header #language #languageBtn').mouseup(function() {
			
			if((this.isDone || false)) return;
			
			jQuery('#languageSelector li ul li a').each(function() {
		
				var tmpHref = this.getAttribute('href') || '';
				
				// Pruefen ob im Link ein Sprachschalter existiert, wenn nicht, dann ignorieren.
				if(tmpHref.indexOf('language=') < 0) return;

				jQuery(this).click(switchLanguage);
			});
		});
	});
	
//-->
</script>







