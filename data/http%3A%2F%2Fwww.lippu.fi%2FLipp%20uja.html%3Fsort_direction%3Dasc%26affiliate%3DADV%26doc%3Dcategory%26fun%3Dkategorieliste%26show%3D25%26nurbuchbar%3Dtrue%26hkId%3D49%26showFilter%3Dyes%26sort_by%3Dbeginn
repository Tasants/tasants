	<!--

		var checkLanguage = function() {

			// Sprache aus dem Webshop-Cookie herausfiltern...
			var tmpArray = (eveJs.base64_decode(getCookie('webshop') || '')).split('&');
			var webshopCookie = {};
			var tmpPair;
					
			for(var i = 0; i < tmpArray.length; i++) {
				tmpPair = tmpArray[i].split('=');
				webshopCookie[tmpPair.shift()] = tmpPair.join('=');
			}
			
			// navigator.language = firefox, webkit, opera
			// navigator.userLanguage = internet explorer
			var languages = {
	
				'supported' : {'fi' : true, 'sv' : true, 'en' : true},
				'current'   : "fi",
				'standard'  : "fi",
				'webshop'   : webshopCookie.language || false,
				'navigator' : ((navigator.language || false) || (navigator.userLanguage || false))
			};
			
			// Wenn Cookie da, dann die Sprache nehmen, wenn nicht dann Navigator-Sprache nehmen, wenn auch nicht,
			// dann Ende.
			var givenLanguage = ((languages.webshop || false) || (languages.navigator || false));
			
			// Wenn das language-cookie gesetzt ist, gewinnt dieses
			var languageCookie = getCookie("language") || false;
			if(languageCookie) givenLanguage = languageCookie;
			
			if(!(givenLanguage || false)) return;

			// Sprache identisch mit aktuellem Template? Wenn ja, dann alles okay!
			if(givenLanguage == languages.current) return;

			// Ist Sprache aus Parameter fuer die Plattform verfuegbar?
			if((languages.supported[givenLanguage] || false)) {

				// Sprache aus Cookie im Template verfuegbar, dann Redirect fuer neue Sprache!
				document.location.href = 'Lippuja.html?affiliate=ADV&language=' + givenLanguage;
				
			} else {
				
				// Wenn nein, dann auf Default weiterleiten, aber nur dann, wenn die aktuelle
				// Sprache nicht schon der Standard-Version entspricht, dann einfach Ende.
				if(languages.standard == languages.current) return;
				
				document.location.href = 'Lippuja.html?affiliate=ADV&language=' + languages.standard;
			}
		};

		checkLanguage();
		
	//-->
	</script>



