																	<div id="bannerTeaserStandardBox1"></div>

<script language="javascript" type="text/javascript">
<!--//
// Copyright 2008 by eventim.de				
/**
 * Globale Bannerrotation, wird genutzt, falls keine Werte an die aktuelle Funktion uebergeben wurden, bzw. wird ueberschrieben
 * wenn bereits zuvor gesetzt ...
 *	- url		Ziel-Url
 *	- img		Bild-Url
 *	- title		Optional: Titel fuer den Banner (alt-Attribut fuer Bild)
 *  - name		Für HBX - Tracking
 *	- target	Optional: Zielfenster (neuer Default: _self)
 */
 
 
 
var globalRotation = [	
		
	









				
						
			{	
				title  : "", 
				target : "_parent", 
				url    : "Lippuja.html?affiliate=ADV", 
				img    : "/obj/media/FI-eventim/sidebar/fi/bannerTeaser/payment-methods.gif",
				seo    : ""
								,
				onclick: "return xt_adc(this, 'INT-1-[sidebar_bannerTeaser_1]||')"
								 } 			];

/** * Filtert abgelaufene oder ungueltige Items aus ... */
var filterBannerRotation = function(items) {	
	var items     = items || [];
	var tmpBanner = [];
	var tmpNow    = new Date();	
	for (var i = 0; i < items.length; i++) {	
		if ((items[i].start || !(items[i].start || false)) <= tmpNow && (items[i].end >= tmpNow || !(items[i].end || false))) tmpBanner.push(items[i]);
	};	
	return (tmpBanner.length == 0) ? false : tmpBanner ;
};

/** * Default Rotation setzen, zuvor, falls gesetzt lokal, diese noch validieren ... */
var defaultRotation1 = filterBannerRotation(defaultRotation1 || globalRotation) || globalRotation;

/** * Banner, die wirklich immer angezeigt werden soll, bzw. in der Rotation sein sollen! */
var everytimeRotation = [];
defaultRotation1 = defaultRotation1.concat(everytimeRotation || []);

/** * Banner, die der normalen Rotation hinzugefuegt werden sollen, sowie die die immer * laufen sollen, nun der defaultRotation hinzufuegen. */
var additionalRotation = additionalRotation || [];
defaultRotation1 = defaultRotation1.concat(additionalRotation);

/** * Erstellt einen Banner aus einer uebergebenen Rotation(wenn verfuegbar). */
var foundBannerRotation = function() {	
	// Werte validieren und vorbereiten ...
	var tmpBanner = filterBannerRotation(defaultRotation1 || []);	
	// Jetzt wo bekannt ist, wieviele Banner noch aktiv in der Rotation sind, kann per Zufall einer davon angezeigt werden,
	// vorrausgesetzt, es gibt mindestens einen.
	if (!tmpBanner) return;
	
	// Syncronisierung mit Bannern
	//
	// Wurde in den Wallpapern der Wert LinkedBanner gesetzt und hier in den Bannern auch,
	// dann duerfen nur die Uebereinstimmungen angezeigt werden.
	var showBanner;
	if(typeof(LinkedBanner) == 'string' && LinkedBanner != "") {
		for (var i = 0; i < tmpBanner.length; i++) {
			if(tmpBanner[i].seo == LinkedBanner){
				showBanner = tmpBanner[i];
			}
		}
	} else {
		var tmpZufall = Math.round((tmpBanner.length - 1) * Math.random());
		showBanner = tmpBanner[tmpZufall] || false;
	}
	if (!showBanner) return;

	// Ausgabe des Banners, wenn einer gefunden wurde ..., darauf achten, dass zwecks Tracking die Session
	// mit durchgereicht wird, falls vorhanden (andernfalls einfach "EVE_NO_SESSION").
	if(showBanner) {

		// Wurde von document.write auf diese Variante umgestellt, da aus unbekannten Gruenden
		// die vorherige Variante als HTML-Tags im Browser gerendert wurden, auch wenn der
		// Quellcode nicht ausgefuehrt wurde (BrowserBug).
		var tmpBoxDiv = document.createElement('div');
		tmpBoxDiv.className = "noBackgroundBannerTeaser";
		
		var tmpBox = document.createElement('div');
		tmpBox.className = "defBox plainBox";
		tmpBoxDiv.appendChild(tmpBox);

		var tmpBody = document.createElement('div');
		tmpBody.className = "body ";
		tmpBox.appendChild(tmpBody);

		var tmpLink = document.createElement('a');
		tmpLink.setAttribute('href', (showBanner.url.replace(/&amp;/gi, "&") || '#'));
		tmpLink.setAttribute('target', (showBanner.target || '_self'));
		tmpLink.setAttribute('name', (showBanner.name || ''));
		tmpLink.setAttribute('onclick', (showBanner.onclick || ''));		tmpBody.appendChild(tmpLink);

		var tmpImage = document.createElement('img');
		tmpImage.setAttribute('src', (showBanner.img || ''));
		tmpImage.setAttribute('alt', (showBanner.title || ''));
		tmpImage.setAttribute('title', (showBanner.title || ''));
		tmpLink.appendChild(tmpImage);

		document.getElementById('bannerTeaserStandardBox1').appendChild(tmpBoxDiv); 
	}	
};

foundBannerRotation(defaultRotation1 || false);
//-->
</script>													</div></div>			</div>
