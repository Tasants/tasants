						<a id="languageBtn" href="javascript:void(0);" class="current" onmouseover="javascript:if(((Eve || {}).DynamicLayers || false)) Eve.DynamicLayers.quickFx(this, { layerEl:'languageSelector', offsetY:1,  offsetX:2 , profile:'clickLayerBottom' });">Suomeksi</a>
			<script type="text/javascript" language="javascript">
				var requests = new Array();
							
			function addHiddenForm(params) {
				
				var form = document.createElement("form");
    			form.setAttribute("method", "post");
    			form.setAttribute("action", "Lippuja.html");
				form.setAttribute("id", "changeLocale");
				
			    for(var key in params) {
			        var hiddenField = document.createElement("input");
			        hiddenField.setAttribute("type", "hidden");
			        hiddenField.setAttribute("name", key);
			        hiddenField.setAttribute("value", params[key]);
			        form.appendChild(hiddenField);
			    }
				
				var languageField = document.createElement("input");
		        languageField.setAttribute("type", "hidden");
		        languageField.setAttribute("name", "language");
		        languageField.setAttribute("id", "changeLocaleLanguage");
		        form.appendChild(languageField);
				
				var affiliateField = document.createElement("input");
		        affiliateField.setAttribute("type", "hidden");
		        affiliateField.setAttribute("name", "affiliate");
		        affiliateField.setAttribute("id", "changeLocaleAffiliate");
		        form.appendChild(affiliateField);
				
				document.body.appendChild(form);
			}
			
			
			function changeLanguage(language, affiliate){
				// Nachschauen ob die parameter, die an die Funktion uebergeben worden,
				// auch wirklich nicht leer sind ..., andernfalls Abbruch.
				if(!(language || false)) return false;
				if(!(affiliate || false)) return false;
			
				console.log(requests);
				//hidden inputs erzeugen
				addHiddenForm(requests);
				
				// Formular ermitteln, wenn es das nicht gibt, Abbruch!
				var tmpForm=document.getElementById("changeLocale") || false;
				if(!tmpForm) return false;
			
				// language-Input ermitteln, wenn es das nicht gibt, Abbruch!
				var tmpEl= document.getElementById("changeLocaleLanguage") || false;
				if(!tmpEl) return false;
		
				// Affiliate-Input ermitteln, wenn es das nicht gibt, Abbruch!
				var tmpAffiliate=document.getElementById("changeLocaleAffiliate") || false;
				if(!tmpAffiliate) return false;
			
				// Die an die Funktion uebergebene Sprache im Input setzen ...
				tmpEl.value = language;
			
				// Die an die Funktion uebergebene Affiliate im Input setzen ...
				tmpAffiliate.value = affiliate;
				
				// Formular nun absenden!
				tmpForm.submit();

				// An den Link false also Abbruch des Clicks senden.
				return false;		
			};
			</script>
		
						
