			var mbetSetUpItemsPerPage = function(id, itemsPerPage) {
			
				var tmpUlList = document.getElementById(id) || false;
				
				// Wenn nix gefunden, dann Ende.
				if (!tmpUlList || !(tmpUlList.childNodes.length || false)) return;
				
				// Merker setzen, wieviele Items bereits angezeigt werden
				var shownItems = 0;
				
				for (var i = 0; i < tmpUlList.childNodes.length; i++) {
					
					var tmpChild = tmpUlList.childNodes[i];
					
					if (tmpChild.nodeName.toUpperCase() != 'LI') continue;
					
					shownItems++;
					
					// Wenn das aktuelle Item genau der Max-Grenze entspricht, dann zumindest die entsprechende "last"-Klasse setzen
					if(shownItems == itemsPerPage) tmpChild.className += ' last';
					
					// Wenn das aktuelle Item bereits schon ueber die Max-Grenze faellt - ausblenden!
					if(shownItems > itemsPerPage) tmpChild.style.display = 'none';
				};
			};
			
				
				mbetSetUpItemsPerPage('mbeteveUniqueId1', 6);
		
